{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "globals": {
        "GM_getValue": "readonly",
        "GM_setValue": "readonly",
        "GM_registerMenuCommand": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "max-len": "off",
        "no-console": "off",
        "no-alert": "off"
    }
}
